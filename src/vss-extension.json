{
  "manifestVersion": 1,
  "id": "autocoder-task",
  "name": "Autocoder Task",
  "publisher": "RaduGheorghiu",
  "description": "Azure DevOps Pipeline Task for AI-powered code generation",
  "categories": [
    "Azure Pipelines",
    "Azure Repos"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "images/icon.png"
  },
  "files": [
    {
      "path": "images"
    },
    {
      "path": "AutoCoderV1/dist"
    }
  ],
  "contributions": [
    {
      "id": "autocoder-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "AutoCoderV1/dist"
      }
    }
  ]
}